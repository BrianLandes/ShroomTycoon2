<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit trunk//EN" "http://google-web-toolkit.googlecode.com/svn/trunk/distro-source/core/src/gwt-module.dtd">
<module rename-to="html">
	<inherits name='com.badlogic.gdx.backends.gdx_backends_gwt' />
	<inherits name='com.badlogic.gdx.physics.box2d.box2d-gwt' />
	<inherits name='aurelienribon.tweenengine'/>

	<inherits name='ShroomClass' />
	<extend-configuration-property name="gdx.reflect.include" value="com.bytesbrothers.shroomtycoon" />
	<extend-configuration-property name="gdx.reflect.include" value="com.bytesbrothers.shroomtycoon.elements" />
	<extend-configuration-property name="gdx.reflect.include" value="com.bytesbrothers.shroomtycoon.pools" />
	<extend-configuration-property name="gdx.reflect.include" value="java.util.Calendar" />
	<extend-configuration-property name="gdx.reflect.include" value="java.lang.reflect.Array" />
	<extend-configuration-property name="gdx.reflect.include" value="easypool.ObjectPool" />
	<extend-configuration-property name="gdx.reflect.include" value="com.bytesbrothers.shroomtycoon.client.HtmlLauncher" />
	<entry-point class='com.bytesbrothers.shroomtycoon.client.HtmlLauncher' />
	<set-configuration-property name='xsiframe.failIfScriptTag' value='FALSE'/>
	<set-configuration-property name="gdx.assetpath" value="../android/assets" />
</module>
